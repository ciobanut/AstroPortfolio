---
const { stacks } = Astro.props;
import Button from "@components/Form/Button.astro";
---

{
	stacks.map((stack: string) => (
		<>
			<Button
				href={"/stacks/" + stack}
				title={stack}
				bg_color="bg-slate-50"
				text_color="text-slate-500 text-sm"
				padding="px-2 py-1 grow-0"
				border="border border-1 border-slate-300 rounded-md"
			/>
		</>
	))
}
